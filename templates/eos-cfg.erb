<% @config.each do |key, val| -%>
<% next if val == :undef or val.nil? -%>
<% if val.is_a?(Array) -%>
<% val.each do |item| -%>
<%= key.to_s %>  <%= item.to_s %>
<% end -%>
<% else -%>
<%= key.to_s %>  <%= val.to_s %>
<% end -%>
<% end -%>

<% if @custom_fragment -%>

<%= @custom_fragment.to_s %>
<% end -%>
